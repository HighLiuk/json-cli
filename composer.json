{
  "name": "swaggest/json-cli",
  "description": "JSON CLI tool (diff, rearrange, pretty print, minify, yaml convert, etc...)",
  "type": "tool",
  "license": "MIT",
  "authors": [
    {
      "name": "Viacheslav Poturaev",
      "email": "vearutop@gmail.com"
    }
  ],
  "require": {
    "swaggest/json-diff": "^3.3.1",
    "php-yaoi/php-yaoi": "^1",
    "symfony/yaml": "^3"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8.35",
    "phpunit/php-code-coverage": "2.2.4",
    "codeclimate/php-test-reporter": "^0.4.0"
  },
  "autoload": {
    "psr-4": {
      "Swaggest\\JsonCli\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Swaggest\\JsonCli\\Tests\\": "tests/src"
    }
  },
  "config": {
    "platform": {
      "php": "5.5.9"
    }
  },
  "bin": [
    "bin/json-cli"
  ]
}
